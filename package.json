{
  "name": "personal",
  "version": "1.0.0",
  "description": "My personal website",
  "author": "Dominic Carrington",
  "private": true,
  "scripts": {
    "dev": "yarn run md2json && nuxt",
    "build": "yarn run md2json && nuxt build",
    "start": "nuxt start",
    "generate": "cross-env-shell API_URL=https://dominiccarrington.github.io \"yarn run md2json && nuxt generate\"",
    "deploy": "yarn run generate && push-dir --dir=dist --branch=master --cleanup",
    "md2json": "m2j -c content/blogposts/*.md -o static/blogposts.json && node postsToTags.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/markdownit": "^1.2.4",
    "@nuxtjs/moment": "^1.2.0",
    "@nuxtjs/pwa": "^2.6.0",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.11",
    "cross-env": "^5.2.0",
    "markdown-it-prism": "^2.0.1",
    "markdown-to-json": "^0.5.1",
    "moment": "^2.24.0",
    "node-sass": "^4.11.0",
    "nuxt": "^2.4.0",
    "nuxt-fontawesome": "^0.4.0",
    "push-dir": "^0.4.1",
    "sass-loader": "^7.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  }
}
